{
  "source": "./src",
  "destination": "./esdoc",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "title": "funclate",
        "index": "./doc/README.md",
        "test": {
          "type": "mocha",
          "source": "./test"
        },
        "manual": {
          "index": "./manual/README.md",
          "globalIndex": true,
          "files": [
            "./manual/installation.md",
            "./manual/usage_templating.md",
            "./manual/usage_runtime.md",
            "./manual/overview.md",
            "./manual/advanced.md",
            "./CHANGELOG.md"
          ]
        }
      }
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {
            "from": "/index.js$",
            "to": ""
          }
        ]
      }
    }
  ]
}
